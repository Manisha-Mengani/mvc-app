<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">

      <h2>Confirm deletion of course</h2>
      <br>
      <br> ID: <%=  course._id %>
      <br> schoolnumber: <%=course.schoolnumber%>
      <br> coursenumber: <%=course.coursenumber%>
      <br> name: <%=course.name%>
      <br> inspring: <%=course.inspring %>
      <br> insummer: <%=course.insummer %>
      <br> infall: <%=course.infall %>
    
      <hr>
      
      <form method="get" action="/course">
        <input type="submit" value="Return to list" class="btn btn-caution" />
      </form>


      <form method="POST" action="/course/delete/<%= course._id %>">
        <input type="submit" value="Delete this item" class="btn btn-danger pull-right" />
      </form>

    </div>
  </div>
</div>